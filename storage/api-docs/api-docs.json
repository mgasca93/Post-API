        "/category/create": {
            "post": {
                "tags": [
                    "Categories"
                ],
                "summary": "Store a new category",
                "operationId": "addCategory",
                "parameters": [
                    {
                        "name": "name",
                        "in": "query",
                        "description": "Name of the catory",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "Tutorial"
                    }
                ],
                "requestBody": {
                    "description": "A JSON object containing category information",
                    "content": {
                        "application/json": {}
                    }
                },
                "responses": {
                    "201": {
                        "description": "Category created with success"
                    },
                    "409": {
                        "description": "Failed to set category"
                    }
                }
            }
        },
        "/category/{slug}": {
            "get": {
                "tags": [
                    "Categories"
                ],
                "summary": "Get category with match slug",
                "operationId": "findCategory",
                "parameters": [
                    {
                        "name": "slug",
                        "in": "path",
                        "description": "Slug of the catory",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "peliculas"
                    }
                ],
                "requestBody": {
                    "description": "A JSON object containing category information",
                    "content": {
                        "application/json": {}
                    }
                },
                "responses": {
                    "200": {
                        "description": "Category retrieved successfully"
                    },
                    "404": {
                        "description": "Failed to get category"
