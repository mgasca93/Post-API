                        "description": "Slug of the catory",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "peliculas"
                    },
                    {
                        "name": "name",
                        "in": "query",
                        "description": "Name of the catory",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "Tutorial"
                    }
                ],
                "requestBody": {
                    "description": "A JSON object containing category updated information",
                    "content": {
                        "application/json": {}
                    }
                },
                "responses": {
                    "200": {
                        "description": "Category updated with success"
                    },
                    "404": {
                        "description": "Category not found"
                    },
                    "409": {
                        "description": "Failed to update category"
                    }
                }
            },
            "delete": {
                "tags": [
                    "Categories"
                ],
                "summary": "Delete category with match slug",
                "operationId": "deleteCategory",
                "parameters": [
                    {
                        "name": "slug",
                        "in": "path",
                        "description": "Slug of the catory",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "peliculas"
                    }
                ],
                "requestBody": {
                    "description": "A JSON object containing confirmation delete category",
                    "content": {
                        "application/json": {}
                    }
                },
                "responses": {
                    "200": {
